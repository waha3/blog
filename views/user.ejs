<%- include header %>
<% post.forEach(function (post, index) { %>
  <p>
    <h2>
      <a href="/u/<%= post.name %>/<%= post.time %>/<%= title %>"><%= title %></a>
    </h2>
  </p>
  <p class="info">
    作者：<a href="/u/<%= post.name %>"><%= post.name %></a>
    日期：<%= post.time %>
  </p>
  <p><%- post.post %></p>
<% }) %>
<%- include footer %>
